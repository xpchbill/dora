{
  "name": "@ALK-UI/core",
  "version": "1.0.0",
  "description": "Responsive and accessible React UI components built with React and Emotion",
  "main": "dist/cjs",
  "module": "dist/esm",
  "typings": "dist/types",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "nodemon --exec yarn build -e ts,tsx --ignore dist/ --ignore src/tests/ --ignore \"*.stories.tsx\"",
    "build": "concurrently yarn:build:*",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist/types",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@ALK-UI/css-reset": "1.0.0",
    "@ALK-UI/hooks": "1.0.0",
    "@ALK-UI/focus-lock": "1.0.0",
    "@ALK-UI/live-region": "1.0.0",
    "@ALK-UI/popper": "1.0.0",
    "@ALK-UI/clickable": "1.0.0",
    "@ALK-UI/color": "1.0.0",
    "@ALK-UI/layout": "1.0.0",
    "@ALK-UI/counter": "1.0.0",
    "@ALK-UI/descendant": "1.0.0",
    "@ALK-UI/portal": "1.0.0",
    "@ALK-UI/system": "1.0.0",
    "@ALK-UI/breadcrumb": "1.0.0",
    "@ALK-UI/collapse": "1.0.0",
    "@ALK-UI/editable": "1.0.0",
    "@ALK-UI/icon": "1.0.0",
    "@ALK-UI/image": "1.0.0",
    "@ALK-UI/media-query": "1.0.0",
    "@ALK-UI/menu": "1.0.0",
    "@ALK-UI/pin-input": "1.0.0",
    "@ALK-UI/progress": "1.0.0",
    "@ALK-UI/slider": "1.0.0",
    "@ALK-UI/tabs": "1.0.0",
    "@ALK-UI/toast": "1.0.0",
    "@ALK-UI/visually-hidden": "1.0.0",
    "@ALK-UI/avatar": "1.0.0",
    "@ALK-UI/checkbox": "1.0.0",
    "@ALK-UI/close-button": "1.0.0",
    "@ALK-UI/form-control": "1.0.0",
    "@ALK-UI/radio": "1.0.0",
    "@ALK-UI/spinner": "1.0.0",
    "@ALK-UI/stat": "1.0.0",
    "@ALK-UI/tag": "1.0.0",
    "@ALK-UI/tooltip": "1.0.0",
    "@ALK-UI/accordion": "1.0.0",
    "@ALK-UI/alert": "1.0.0",
    "@ALK-UI/button": "1.0.0",
    "@ALK-UI/input": "1.0.0",
    "@ALK-UI/modal": "1.0.0",
    "@ALK-UI/number-input": "1.0.0",
    "@ALK-UI/popover": "1.0.0",
    "@ALK-UI/select": "1.0.0",
    "@ALK-UI/switch": "1.0.0",
    "@ALK-UI/textarea": "1.0.0",
    "@ALK-UI/alert-dialog": "1.0.0",
    "@ALK-UI/drawer": "1.0.0",
    "@ALK-UI/skeleton": "1.0.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "license": "MIT",
